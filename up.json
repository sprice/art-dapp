{
  "name": "chillist",
  "profile": "up",
  "regions": ["us-east-1"],
  "hooks": {
    "build": "npm run build"
  },
  "type": "static",
  "static": {
    "dir": "build_webpack"
  },
  "stages": {
    "production": {
      "domain": "chill.ist"
    },
    "staging": {
      "domain": "ropsten.chill.ist"
    }
  },
  "dns": {
    "chill.ist": [
      {
        "name": "chill.ist",
        "type": "NS",
        "value": [
          "ns-1048.awsdns-03.org",
          "ns-16.awsdns-02.com",
          "ns-1711.awsdns-21.co.uk",
          "ns-726.awsdns-26.net"
        ]
      }
    ]
  }
}