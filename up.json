{
  "name": "chillist",
  "profile": "up",
  "regions": [
    "us-east-1"
  ],
  "lambda": {
    "timeout": 10
  },
  "hooks": {
    "build": "npm run build; browserify --node app.js > server.js",
    "clean": "npm run clean"
  },
  "stages": {
    "production": {
      "domain": "chill.ist"
    }
  }
}